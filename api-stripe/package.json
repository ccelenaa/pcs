  "description": "Api payment",
  "main": "index.ts",
  "scripts": {
    "pretest": "npm run build:ts",
    "test": "",
    "test:lint": "tslint -c tslint.json src/**/*.ts",
    "lint:fix": "tslint --fix --project tsconfig.json",
    "test:integration": "NODE_ENV=test cucumber-run test/scripts.json",
    "build": "npm run build:ts",
    "build:ts": "tsc",
    "tscv": "tsc -v",
    "build:doc": "node swagger.js makedoc",

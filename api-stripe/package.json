    "dev": "nodemon -e ts --exec \"npm run start:ts\"",
    "debug": "npm run build:ts && node --inspect lib/index.js 2>&1",
    "version": "chlg release $npm_package_version && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"

    "@types/restify-errors": "^4.3.2",
    "husky": "^1.3.1",
    "nodemon": "^2.0.15",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.0.0-alpha.4",
    "tslint": "^5.8.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@types/amqplib": "^0.10.1",
